{
        "type": "config",
        "id": "conf.service.drone",
        "title": "Drone",
        "queryinfo": {
                "xpath": ".//services/drone",
                "iterable": false
        },
        "properties": {
                "enable": {
                        "type": "boolean",
                        "default": false
                },
                "composepath": {
                        "type": "string",
                        "default": "/srv/dev-disk-by-label-data/drone",
                        "pattern": "^/.+"
                },
                "timezone": {
                        "type": "string",
                        "default": ""
                },
                "serverport": {
                        "type": "integer",
                        "minimum": 1,
                        "maximum": 65535,
                        "default": 8080
                }
        }
}
